{
  "name": "urban-nav-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "envs": "node ./scripts/set-envs.js",
    "start": "ng serve -o",
    "build": "node ./scripts/set-envs.js && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "format": "prettier --write \"src/**/*.{html,ts}\"",
    "add": "prettier --write \"src/**/*.{html,ts}\" && git add .",
    "css": "npx tailwindcss -i ./src/input.css -o ./src/output.css --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/cdk": "^17.0.1",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/material": "^17.0.1",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "gsap": "^3.12.2",
    "mapbox-gl": "^3.0.0",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.10.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.0",
    "@angular/cli": "^17.0.0",
    "@angular/compiler-cli": "^17.0.0",
    "@types/gsap": "^3.0.0",
    "@types/jasmine": "~5.1.0",
    "@types/mapbox-gl": "^2.7.19",
    "dotenv": "^16.3.1",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "tailwindcss": "^3.3.5",
    "typescript": "~5.2.2"
  }
}
